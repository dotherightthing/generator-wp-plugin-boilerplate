{
  "private": true,
  "name": "<%= name %>",
  "version": "<%= defaultVersion %>",
  "description": "<%= description %>",
  "main": "<%= name %>.php",
  "person": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "https://profiles.wordpress.org/<%= authorWordPressName %>"
  },
  "contributors": [
    {
      "name": "<%= authorName %>",
      "email": "<%= authorEmail %>",
      "url": "https://profiles.wordpress.org/<%= authorWordPressName %>"
    }
  ],
  "bugs": {
    "url": "https://github.com/<%= githubUserName %>/<%= name %>/issues"
  },
  "keywords": <%- pluginKeywords %>,
  "workspaces": [
    "vendor/dotherightthing/wpdtrt-plugin-boilerplate/"
  ],
  "scripts": {
    "build": "./node_modules/.bin/gulp --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./",
    "dependencies": "./node_modules/.bin/gulp dependencies --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./",
    "docs": "./node_modules/.bin/gulp docs --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./",
    "installplugin": "./install.sh",
    "lint": "./node_modules/.bin/gulp lint --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./",
    "release": "./node_modules/.bin/gulp release --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./",
    "test": "./node_modules/.bin/gulp test --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./ && cypress open",
    "version": "./node_modules/.bin/gulp version --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./",
    "watch": "./node_modules/.bin/gulp compile --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./ && gulp watch --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile-loader.js --cwd ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUserName %>/<%= name %>"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "license": "ISC",
  "homepage": "https://github.com/<%= githubUserName %>/<%= name %>",
  "srcDir": "/Volumes/DanBackup/Websites/<%= name %>"
}
