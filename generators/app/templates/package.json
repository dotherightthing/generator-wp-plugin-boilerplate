{
  "private": true,
  "name": "<%= name %>",
  "version": "<%= defaultVersion %>",
  "description": "<%= description %>",
  "main": "<%= name %>.php",
  "person": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "https://profiles.wordpress.org/<%= authorWordPressName %>"
  },
  "contributors": [
    {
      "name": "<%= authorName %>",
      "email": "<%= authorEmail %>",
      "url": "https://profiles.wordpress.org/<%= authorWordPressName %>"
    }
  ],
  "bugs": {
    "url" : "https://github.com/<%= githubUserName %>/<%= name %>/issues"
  },
  "keywords": <%- pluginKeywords %>,
  "devDependencies": {
    "gulp": "^3.9.1"
  },
  "workspaces": [
    "vendor/dotherightthing/wpdtrt-plugin-boilerplate/"
  ],
  "scripts": {
    "build": "gulp --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./",
    "install_deps": "gulp install_dependencies --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./ && gulp wpunit_install --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./",
    "package": "gulp docs --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./ && gulp release --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./",
    "lint": "gulp lint --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./",
    "test": "gulp wpunit_run --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./",
    "version": "gulp version --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./",
    "watch": "gulp compile --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./ && gulp watch --gulpfile ./vendor/dotherightthing/wpdtrt-plugin-boilerplate/gulpfile.js --cwd ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUserName %>/<%= name %>"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "license": "ISC",
  "homepage": "https://github.com/<%= githubUserName %>/<%= name %>",
  "srcDir": "/Volumes/DanBackup/Websites/<%= name %>"
}
