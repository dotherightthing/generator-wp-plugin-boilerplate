{
	"name": "<%= authorWordPressName %>/<%= name %>",
	"description": "<%= description %>",
	"type": "library",
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "vendor"
    },
    "authors": [
        {
            "name": "<%= authorName %>",
            "email": "<%= authorEmail %>"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:dotherightthing/wpdtrt-debug.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:dotherightthing/wpdtrt-plugin.git"
        }
    ],
	"require-dev": {
        "phpunit/phpunit": "^5.7",
        "wp-cli/wp-cli" : "~0.22",
        "psy/psysh" : "~0.6",
        "phpdocumentor/phpdocumentor": "2.*",
        "dotherightthing/wpdtrt-debug": "dev-master"
	},
    "require": {
        "php": ">=5.6.30",
        "dotherightthing/wpdtrt-plugin": "dev-master"
    }
}
