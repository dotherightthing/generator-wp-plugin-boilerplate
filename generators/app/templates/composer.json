{
	"name": "<%= githubUserName %>/<%= name %>",
	"description": "<%= description %>",
	"type": "library",
    "keywords": <%- pluginKeywords %>,
    "license": "ISC",
    "authors": [
        {
            "name": "<%= authorName %>",
            "email": "<%= authorEmail %>",
            "homepage": "<%= homepage %>",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "<%= authorSupportEmail %>",
        "issues": "https://github.com/<%= githubUserName %>/<%= name %>/issues"
    },
    "require": {
        "dotherightthing/wpdtrt-plugin": "1.2.20"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/dotherightthing/"
        },
        {
            "packagist.org": false
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    }
}
